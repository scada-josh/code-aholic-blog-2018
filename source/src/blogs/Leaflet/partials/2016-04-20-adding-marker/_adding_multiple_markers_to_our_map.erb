for (var i = 0; i < planes.length; i++) {
		tmpMarker = new L.marker([planes[i][1],planes[i][2]])
                      .bindPopup(planes[i][0])
                      // .addTo(map);
        planesArr.push(tmpMarker)
}

var planes_group = L.layerGroup(planesArr);

map.addLayer(planes_group);</mark>

